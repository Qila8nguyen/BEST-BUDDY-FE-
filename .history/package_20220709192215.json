{
  "name": "cloudverse-notification-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && source .env.local && next dev -p $APP_PORT",
    "start:dev": "source ./.env.local && npm run prisma:generate && PORT=$APP_PORT next dev",
    "generate": "npm -s run prisma:generate && npm -s run generate:nexus",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api/graphql",
    "prisma:generate": "prisma generate",
    "studio": "prisma studio",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@prisma/client": "^3.15.2",
    "@reduxjs/toolkit": "^1.8.0",
    "@svgr/webpack": "^6.2.1",
    "@types/email-templates": "^8.0.4",
    "@types/micro": "^7.3.7",
    "@types/micro-cors": "^0.1.2",
    "@types/nodemailer": "^6.4.4",
    "@types/react-redux": "^7.1.23",
    "antd": "4.20.6",
    "apollo-server-micro": "^3.9.0",
    "axios": "^0.26.1",
    "email-templates": "^9.0.0",
    "fs": "^0.0.1-security",
    "graphql": "^16.5.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "mongoose": "^6.2.8",
    "next": "^12.1.0",
    "next-auth": "^4.3.1",
    "next-redux-wrapper": "^7.0.5",
    "nexus": "^1.3.0",
    "nodemailer": "^6.7.5",
    "prisma": "^3.15.2",
    "rc-menu": "^9.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "sass": "^1.49.9",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.43",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
